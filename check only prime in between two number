#include<stdio.h>
int main(){
    int a,b,i,j;
    printf("enter the number : ");
    scanf("%d %d",&a,&b);
    for(i=a;i<=b;i++){
        if(i<=0)
        continue;
        for(j=2;j*j<=i;j++){
            if(i%j==0)
            break;
        }
        if(j*j>i){
            printf("%d\n",i);
        }
        
    }
    return 0;
}
